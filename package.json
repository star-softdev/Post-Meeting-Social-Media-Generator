{
  "name": "post-meeting-social-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:security": "jest --testPathPattern=security",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:all": "npm run test && npm run test:integration && npm run test:performance && npm run test:security",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "deploy": "bash scripts/deploy.sh",
    "docker:build": "docker build -f Dockerfile.prod -t post-meeting-app .",
    "docker:run": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.prod.yml down",
    "docker:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "health": "curl -f http://localhost:3000/api/health",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "@next-auth/prisma-adapter": "^1.0.7",
    "next-auth": "^4.24.5",
    "@auth/prisma-adapter": "^1.0.12",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "googleapis": "^128.0.0",
    "axios": "^1.6.2",
    "openai": "^4.20.1",
    "lucide-react": "^0.294.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@tailwindcss/forms": "^0.5.7",
    "react-hook-form": "^7.48.2",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2",
    "date-fns": "^2.30.0",
    "react-hot-toast": "^2.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "recharts": "^2.8.0",
    "prom-client": "^15.0.0",
    "ioredis": "^5.3.2",
    "@slack/web-api": "^6.10.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "swagger-ui-react": "^5.9.0",
    "swagger-jsdoc": "^6.2.8",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
