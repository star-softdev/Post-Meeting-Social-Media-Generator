    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "supertest": "^6.3.3",
    "node-mocks-http": "^1.13.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "prettier": "^3.1.0"
  }
